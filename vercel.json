{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python@3.11"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ]
}