{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "HANDLER": "api/main.app"
    }
}